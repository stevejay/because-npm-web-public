{
  "name": "because-npm-web-public",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/recompose": "^0.30.0",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "downshift": "^2.2.2",
    "global": "^4.3.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.11",
    "loglevel": "^1.6.1",
    "material-colors": "^1.2.6",
    "mitt": "^1.1.3",
    "normalize.css": "^8.0.0",
    "react": "^16.5.0",
    "react-apollo": "^2.3.1",
    "react-content-loader": "^3.2.0",
    "react-delay": "^0.1.0",
    "react-dom": "^16.5.0",
    "react-icons": "^3.0.5",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "recompact": "^3.4.0",
    "sass-material-colors": "^0.0.5",
    "what-input": "^5.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "HTTPS=true react-app-rewired start --scripts-version react-scripts-ts",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build --scripts-version react-scripts-ts",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint:fix": "tslint --fix --project .",
    "build:serve": "./node_modules/.bin/ws --directory ./build --port 3000 --https --spa index.html",
    "deploy:prod": "./node_modules/.bin/firebase deploy --project because-npm-prod-public"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/loglevel": "^1.5.3",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-content-loader": "^3.1.3",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "firebase-tools": "^6.1.1",
    "local-web-server": "^2.6.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-css-modules-extensionless": "^1.2.0",
    "react-app-rewire-lodash": "^2.0.0",
    "react-app-rewired": "^1.6.2",
    "typescript": "^3.0.3"
  }
}
